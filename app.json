{
  "expo": {
    "name": "Restaurant",
    "slug": "Restaurant",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo.png",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.saudch00.Restaurant"
    },
    "web": {
      "favicon": "./assets/img/logo.png"
    }
  }
}
